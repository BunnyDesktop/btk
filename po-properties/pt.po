#: ../bdk/bdkdisplaymanager.c:103
msgid "Default Display"
msgstr "Ecrã por Omissão"

#: ../bdk/bdkdisplaymanager.c:104
msgid "The default display for BDK"
msgstr "O ecrã por omissão do BDK"

#: ../bdk/bdkbango.c:538 ../btk/btkinvisible.c:86
#: ../btk/btkmountoperation.c:176 ../btk/btkstatusicon.c:280
#: ../btk/btkwindow.c:641
msgid "Screen"
msgstr "Ecrã"

#: ../bdk/bdkbango.c:539
msgid "the BdkScreen for the renderer"
msgstr "O BdkScreen para o renderizador"

#: ../bdk/bdkscreen.c:75
msgid "Font options"
msgstr "Opções de fonte"

#: ../bdk/bdkscreen.c:76
msgid "The default font options for the screen"
msgstr "As opções de fonte por omissão para o ecrã"

#: ../bdk/bdkscreen.c:83
msgid "Font resolution"
msgstr "Resolução da fonte"

#: ../bdk/bdkscreen.c:84
msgid "The resolution for fonts on the screen"
msgstr "A resolução das fontes no ecrã"

#: ../bdk/bdkwindow.c:496 ../bdk/bdkwindow.c:497
msgid "Cursor"
msgstr "Cursor"

#: ../btk/btkaboutdialog.c:298
msgid "Program name"
msgstr "Nome da aplicação"

#: ../btk/btkaboutdialog.c:299
msgid ""
"The name of the program. If this is not set, it defaults to "
"g_get_application_name()"
msgstr ""
"O nome da aplicação. Se não estiver definido, por omissão será "
"g_get_application_name()"

#: ../btk/btkaboutdialog.c:313
msgid "Program version"
msgstr "Versão da aplicação"

#: ../btk/btkaboutdialog.c:314
msgid "The version of the program"
msgstr "A versão da aplicação"

#: ../btk/btkaboutdialog.c:328
msgid "Copyright string"
msgstr "Declaração de copyright"

#: ../btk/btkaboutdialog.c:329
msgid "Copyright information for the program"
msgstr "Informação de copyright da aplicação"

#: ../btk/btkaboutdialog.c:346
msgid "Comments string"
msgstr "Texto de comentários"

#: ../btk/btkaboutdialog.c:347
msgid "Comments about the program"
msgstr "Comentários sobre a aplicação"

#: ../btk/btkaboutdialog.c:381
msgid "Website URL"
msgstr "URL da Página Web"

#: ../btk/btkaboutdialog.c:382
msgid "The URL for the link to the website of the program"
msgstr "O URL do link para a página web da aplicação"

#: ../btk/btkaboutdialog.c:397
msgid "Website label"
msgstr "Etiqueta da página web"

#: ../btk/btkaboutdialog.c:398
msgid ""
"The label for the link to the website of the program. If this is not set, it "
"defaults to the URL"
msgstr ""
"A etiqueta para o link para a página web da aplicação. Se não estiver "
"definida, por omissão será o URL"

#: ../btk/btkaboutdialog.c:414
msgid "Authors"
msgstr "Autores"

#: ../btk/btkaboutdialog.c:415
msgid "List of authors of the program"
msgstr "Uma lista com os autores da aplicação"

#: ../btk/btkaboutdialog.c:431
msgid "Documenters"
msgstr "Documentadores"

#: ../btk/btkaboutdialog.c:432
msgid "List of people documenting the program"
msgstr "Lista de pessoas que elaboraram a documentação da aplicação"

#: ../btk/btkaboutdialog.c:448
msgid "Artists"
msgstr "Artistas"

#: ../btk/btkaboutdialog.c:449
msgid "List of people who have contributed artwork to the program"
msgstr "Lista de pessoas que contribuíram com arte para a aplicação"

#: ../btk/btkaboutdialog.c:466
msgid "Translator credits"
msgstr "Créditos de tradução"

#: ../btk/btkaboutdialog.c:467
msgid ""
"Credits to the translators. This string should be marked as translatable"
msgstr ""
"Créditos para os tradutores. Esta expressão deverá ser marcada como "
"traduzível"

#: ../btk/btkaboutdialog.c:482
msgid "Logo"
msgstr "Logotipo"

#: ../btk/btkaboutdialog.c:483
msgid ""
"A logo for the about box. If this is not set, it defaults to "
"btk_window_get_default_icon_list()"
msgstr ""
"Um logotipo para a caixa \"sobre\". Se não estiver definido, por omissão "
"será o resultado de btk_window_get_default_icon_list()"

#: ../btk/btkaboutdialog.c:498
msgid "Logo Icon Name"
msgstr "Nome do Ícone de Logotipo"

#: ../btk/btkaboutdialog.c:499
msgid "A named icon to use as the logo for the about box."
msgstr "Um nome de ícone a utilizar como logotipo para a caixa \"sobre\"."

#: ../btk/btkaboutdialog.c:512
msgid "Wrap license"
msgstr "Quebra de linha na licença"

#: ../btk/btkaboutdialog.c:513
msgid "Whether to wrap the license text."
msgstr "Se quebrar ou não o texto da licença."

#: ../btk/btkaccellabel.c:178
msgid "Accelerator Closure"
msgstr "Área de Atalho"

#: ../btk/btkaccellabel.c:179
msgid "The closure to be monitored for accelerator changes"
msgstr "A área a ser monitorizada para alterações de atalho"

#: ../btk/btkaccellabel.c:185
msgid "Accelerator Widget"
msgstr "Widget Atalho"

#: ../btk/btkaccellabel.c:186
msgid "The widget to be monitored for accelerator changes"
msgstr "O widget a ser monitorizado para alterações de atalho"

#: ../btk/btkaction.c:220 ../btk/btkactiongroup.c:170 ../btk/btkprinter.c:111
#: ../btk/btktextmark.c:89
msgid "Name"
msgstr "Nome"

#: ../btk/btkaction.c:221
msgid "A unique name for the action."
msgstr "Um nome único para a acção."

#: ../btk/btkaction.c:239 ../btk/btkbutton.c:219 ../btk/btkexpander.c:197
#: ../btk/btkframe.c:105 ../btk/btklabel.c:507 ../btk/btkmenuitem.c:305
#: ../btk/btktoolbutton.c:204 ../btk/btktoolitemgroup.c:1543
msgid "Label"
msgstr "Etiqueta"

#: ../btk/btkaction.c:240
msgid "The label used for menu items and buttons that activate this action."
msgstr ""
"A etiqueta utilizada para itens de menu e botões que activam esta acção."

#: ../btk/btkaction.c:256
msgid "Short label"
msgstr "Etiqueta abreviada"

#: ../btk/btkaction.c:257
msgid "A shorter label that may be used on toolbar buttons."
msgstr ""
"Uma etiqueta abreviada que poderá ser utilizada em botões de barra de "
"ferramentas."

#: ../btk/btkaction.c:265
msgid "Tooltip"
msgstr "Dica"

#: ../btk/btkaction.c:266
msgid "A tooltip for this action."
msgstr "Uma dica para esta acção."

#: ../btk/btkaction.c:281
msgid "Stock Icon"
msgstr "Ícone Base"

#: ../btk/btkaction.c:282
msgid "The stock icon displayed in widgets representing this action."
msgstr "O ícone base apresentado em widgets que representem esta acção."

#: ../btk/btkaction.c:302 ../btk/btkstatusicon.c:253
msgid "GIcon"
msgstr "GIcon"

#: ../btk/btkaction.c:303 ../btk/btkcellrendererpixbuf.c:206
#: ../btk/btkimage.c:341 ../btk/btkstatusicon.c:254
msgid "The GIcon being displayed"
msgstr "O GIcon a ser apresentado"

#: ../btk/btkaction.c:323 ../btk/btkcellrendererpixbuf.c:171
#: ../btk/btkimage.c:323 ../btk/btkprinter.c:160 ../btk/btkstatusicon.c:237
#: ../btk/btkwindow.c:633
msgid "Icon Name"
msgstr "Nome do Ícone"

#: ../btk/btkaction.c:324 ../btk/btkcellrendererpixbuf.c:172
#: ../btk/btkimage.c:324 ../btk/btkstatusicon.c:238
msgid "The name of the icon from the icon theme"
msgstr "O nome do ícone do tema de ícones"

#: ../btk/btkaction.c:331 ../btk/btktoolitem.c:192
msgid "Visible when horizontal"
msgstr "Visível quando horizontal"

#: ../btk/btkaction.c:332 ../btk/btktoolitem.c:193
msgid ""
"Whether the toolbar item is visible when the toolbar is in a horizontal "
"orientation."
msgstr ""
"Se o item de barra de ferramentas é visível quando a barra de ferramentas "
"tem uma orientação horizontal"

#: ../btk/btkaction.c:347
msgid "Visible when overflown"
msgstr "Visível quando a transbordar"

#: ../btk/btkaction.c:348
msgid ""
"When TRUE, toolitem proxies for this action are represented in the toolbar "
"overflow menu."
msgstr ""
"Quando VERDADEIRO, proxies de item de menu para esta acção são representadas "
"no menu de transbordo da barra de ferramentas."

#: ../btk/btkaction.c:355 ../btk/btktoolitem.c:199
msgid "Visible when vertical"
msgstr "Visível quando vertical"

#: ../btk/btkaction.c:356 ../btk/btktoolitem.c:200
msgid ""
"Whether the toolbar item is visible when the toolbar is in a vertical "
"orientation."
msgstr ""
"Se o item de barra de ferramentas é visível quando a barra de ferramentas "
"tem uma orientação vertical"

#: ../btk/btkaction.c:363 ../btk/btktoolitem.c:206
msgid "Is important"
msgstr "É importante"

#: ../btk/btkaction.c:364
msgid ""
"Whether the action is considered important. When TRUE, toolitem proxies for "
"this action show text in BTK_TOOLBAR_BOTH_HORIZ mode."
msgstr ""
"Se a acção é ou não considerada importante. Quando VERDADEIRO, proxies de "
"item de ferramenta para esta acção apresentam texto em modo "
"BTK_TOOLBAR_BOTH_HORIZ."

#: ../btk/btkaction.c:372
msgid "Hide if empty"
msgstr "Esconder se vazio"

#: ../btk/btkaction.c:373
msgid "When TRUE, empty menu proxies for this action are hidden."
msgstr ""
"Quando VERDADEIRO, proxies de menu vazio para esta acção são escondidas."

#: ../btk/btkaction.c:379 ../btk/btkactiongroup.c:177
#: ../btk/btkcellrenderer.c:193 ../btk/btkwidget.c:594
msgid "Sensitive"
msgstr "Sensível"

#: ../btk/btkaction.c:380
msgid "Whether the action is enabled."
msgstr "Se a acção está ou não activa."

#: ../btk/btkaction.c:386 ../btk/btkactiongroup.c:184
#: ../btk/btkstatusicon.c:303 ../btk/btktreeviewcolumn.c:192
#: ../btk/btkwidget.c:587
msgid "Visible"
msgstr "Visível"

#: ../btk/btkaction.c:387
msgid "Whether the action is visible."
msgstr "Se a acção está ou não visível."

#: ../btk/btkaction.c:393
msgid "Action Group"
msgstr "Grupo de Acção"

#: ../btk/btkaction.c:394
msgid ""
"The BtkActionGroup this BtkAction is associated with, or NULL (for internal "
"use)."
msgstr ""
"O BtkActionGroup a que este BtkAction está associado, ou NULL (para "
"utilização interna)."

#: ../btk/btkaction.c:412 ../btk/btkimagemenuitem.c:169
msgid "Always show image"
msgstr "Apresentar sempre a imagem"

#: ../btk/btkaction.c:413 ../btk/btkimagemenuitem.c:170
msgid "Whether the image will always be shown"
msgstr "Se a imagem será ou não sempre visível"

#: ../btk/btkactiongroup.c:171
msgid "A name for the action group."
msgstr "Um nome para o grupo da acção."

#: ../btk/btkactiongroup.c:178
msgid "Whether the action group is enabled."
msgstr "Se o grupo de acção está ou não activo."

#: ../btk/btkactiongroup.c:185
msgid "Whether the action group is visible."
msgstr "Se o grupo de acção está ou não visível."

#: ../btk/btkactivatable.c:308
msgid "Related Action"
msgstr "Acção Relacionada"

#: ../btk/btkactivatable.c:309
msgid "The action this activatable will activate and receive updates from"
msgstr ""
"A acção que este activável vai activar e da qual vai receber actualizações"

#: ../btk/btkactivatable.c:331
msgid "Use Action Appearance"
msgstr "Utilizar a Aparência da Acção"

#: ../btk/btkactivatable.c:332
msgid "Whether to use the related actions appearance properties"
msgstr "Se utilizar as propriedades da aparência das acções relacionadas"

#: ../btk/btkadjustment.c:93 ../btk/btkcellrendererprogress.c:128
#: ../btk/btkscalebutton.c:206 ../btk/btkspinbutton.c:269
msgid "Value"
msgstr "Valor"

#: ../btk/btkadjustment.c:94
msgid "The value of the adjustment"
msgstr "O valor do ajustamento"

#: ../btk/btkadjustment.c:110
msgid "Minimum Value"
msgstr "Valor Mínimo"

#: ../btk/btkadjustment.c:111
msgid "The minimum value of the adjustment"
msgstr "O valor mínimo do ajustamento"

#: ../btk/btkadjustment.c:130
msgid "Maximum Value"
msgstr "Valor Máximo"

#: ../btk/btkadjustment.c:131
msgid "The maximum value of the adjustment"
msgstr "O valor máximo do ajustamento"

#: ../btk/btkadjustment.c:147
msgid "Step Increment"
msgstr "Intervalo de Incremento"

#: ../btk/btkadjustment.c:148
msgid "The step increment of the adjustment"
msgstr "O intervalo de incremento do ajustamento"

#: ../btk/btkadjustment.c:164
msgid "Page Increment"
msgstr "Incremento de Página"

#: ../btk/btkadjustment.c:165
msgid "The page increment of the adjustment"
msgstr "O incremento de página do ajustamento"

#: ../btk/btkadjustment.c:184
msgid "Page Size"
msgstr "Tamanho da Página"

#: ../btk/btkadjustment.c:185
msgid "The page size of the adjustment"
msgstr "O tamanho da página do ajustamento"

#: ../btk/btkalignment.c:109
msgid "Horizontal alignment"
msgstr "Alinhamento horizontal"

#: ../btk/btkalignment.c:110 ../btk/btkbutton.c:270
msgid ""
"Horizontal position of child in available space. 0.0 is left aligned, 1.0 is "
"right aligned"
msgstr ""
"Posição horizontal do filho no espaço disponível. 0.0 é alinhado à esquerda, "
"1.0 é alinhado à direita"

#: ../btk/btkalignment.c:119
msgid "Vertical alignment"
msgstr "Alinhamento vertical"

#: ../btk/btkalignment.c:120 ../btk/btkbutton.c:289
msgid ""
"Vertical position of child in available space. 0.0 is top aligned, 1.0 is "
"bottom aligned"
msgstr ""
"Posição vertical do filho no espaço disponível. 0.0 é alinhado ao topo, 1.0 "
"é alinhado ao fundo"

#: ../btk/btkalignment.c:128
msgid "Horizontal scale"
msgstr "Escala horizontal"

#: ../btk/btkalignment.c:129
msgid ""
"If available horizontal space is bigger than needed for the child, how much "
"of it to use for the child. 0.0 means none, 1.0 means all"
msgstr ""
"Se o espaço horizontal disponível é maior do que o necessário para o filho, "
"quanto utilizar. 0.0 significa nenhum, 1.0 significa todo"

#: ../btk/btkalignment.c:137
msgid "Vertical scale"
msgstr "Escala vertical"

#: ../btk/btkalignment.c:138
msgid ""
"If available vertical space is bigger than needed for the child, how much of "
"it to use for the child. 0.0 means none, 1.0 means all"
msgstr ""
"Se o espaço vertical disponível é maior do que o necessário para o filho, "
"quanto utilizar. 0.0 significa nenhum, 1.0 significa todo"

#: ../btk/btkalignment.c:155
msgid "Top Padding"
msgstr "Espaçamento no Topo"

#: ../btk/btkalignment.c:156
msgid "The padding to insert at the top of the widget."
msgstr "O espaçamento a inserir no topo do filho."

#: ../btk/btkalignment.c:172
msgid "Bottom Padding"
msgstr "Espaçamento no Fundo"

#: ../btk/btkalignment.c:173
msgid "The padding to insert at the bottom of the widget."
msgstr "O espaçamento a inserir no fundo do filho."

#: ../btk/btkalignment.c:189
msgid "Left Padding"
msgstr "Espaçamento à Esquerda"

#: ../btk/btkalignment.c:190
msgid "The padding to insert at the left of the widget."
msgstr "O espaçamento a inserir à esquerda do filho."

#: ../btk/btkalignment.c:206
msgid "Right Padding"
msgstr "Espaçamento à Direita"

#: ../btk/btkalignment.c:207
msgid "The padding to insert at the right of the widget."
msgstr "O espaçamento a inserir à direita do filho."

#: ../btk/btkarrow.c:95
msgid "Arrow direction"
msgstr "Direcção da seta"

#: ../btk/btkarrow.c:96
msgid "The direction the arrow should point"
msgstr "A direcção em que a seta deverá apontar"

#: ../btk/btkarrow.c:104
msgid "Arrow shadow"
msgstr "Sombra da seta"

#: ../btk/btkarrow.c:105
msgid "Appearance of the shadow surrounding the arrow"
msgstr "Aparência da sombra à volta da seta"

#: ../btk/btkarrow.c:112 ../btk/btkmenu.c:717 ../btk/btkmenuitem.c:368
msgid "Arrow Scaling"
msgstr "Escala da Seta"

#: ../btk/btkarrow.c:113
msgid "Amount of space used up by arrow"
msgstr "Quantidade de espaço utilizado pela seta"

#: ../btk/btkaspectframe.c:93
msgid "Horizontal Alignment"
msgstr "Alinhamento Horizontal"

#: ../btk/btkaspectframe.c:94
msgid "X alignment of the child"
msgstr "Alinhamento X do filho"

#: ../btk/btkaspectframe.c:100
msgid "Vertical Alignment"
msgstr "Alinhamento Vertical"

#: ../btk/btkaspectframe.c:101
msgid "Y alignment of the child"
msgstr "Alinhamento Y do filho"

#: ../btk/btkaspectframe.c:107
msgid "Ratio"
msgstr "Rácio"

#: ../btk/btkaspectframe.c:108
msgid "Aspect ratio if obey_child is FALSE"
msgstr "Rácio de aparência se obey_child for FALSO"

#: ../btk/btkaspectframe.c:114
msgid "Obey child"
msgstr "Obedecer ao filho"

#: ../btk/btkaspectframe.c:115
msgid "Force aspect ratio to match that of the frame's child"
msgstr "Forçar o rácio de aparência para coincidir com o da moldura do filho"

#: ../btk/btkassistant.c:308
msgid "Header Padding"
msgstr "Espaçamento do Cabeçalho"

#: ../btk/btkassistant.c:309
msgid "Number of pixels around the header."
msgstr "Número de pixels em torno do cabeçalho."

#: ../btk/btkassistant.c:316
msgid "Content Padding"
msgstr "Espaçamento do Conteúdo"

#: ../btk/btkassistant.c:317
msgid "Number of pixels around the content pages."
msgstr "Número de pixels em torno das páginas de conteúdo."

#: ../btk/btkassistant.c:333
msgid "Page type"
msgstr "Tipo de página"

#: ../btk/btkassistant.c:334
msgid "The type of the assistant page"
msgstr "O tipo de página de assistente"

#: ../btk/btkassistant.c:351
msgid "Page title"
msgstr "Título da página"

#: ../btk/btkassistant.c:352
msgid "The title of the assistant page"
msgstr "O título da página de assistente"

#: ../btk/btkassistant.c:368
msgid "Header image"
msgstr "Imagem de cabeçalho"

#: ../btk/btkassistant.c:369
msgid "Header image for the assistant page"
msgstr "Imagem de cabeçalho para a página de assistente"

#: ../btk/btkassistant.c:385
msgid "Sidebar image"
msgstr "Imagem da barra lateral"

#: ../btk/btkassistant.c:386
msgid "Sidebar image for the assistant page"
msgstr "Imagem da barra lateral da página de assistente"

#: ../btk/btkassistant.c:401
msgid "Page complete"
msgstr "Página terminada"

#: ../btk/btkassistant.c:402
msgid "Whether all required fields on the page have been filled out"
msgstr "Se todos os campos obrigatórios da página foram ou não preenchidos"

#: ../btk/btkbbox.c:101
msgid "Minimum child width"
msgstr "Largura mínima do filho"

#: ../btk/btkbbox.c:102
msgid "Minimum width of buttons inside the box"
msgstr "Largura mínima dos botões dentro da caixa"

#: ../btk/btkbbox.c:110
msgid "Minimum child height"
msgstr "Altura mínima do filho"

#: ../btk/btkbbox.c:111
msgid "Minimum height of buttons inside the box"
msgstr "Altura mínima dos botões dentro da caixa"

#: ../btk/btkbbox.c:119
msgid "Child internal width padding"
msgstr "Largura de espaçamento interno do filho"

#: ../btk/btkbbox.c:120
msgid "Amount to increase child's size on either side"
msgstr "Quanto aumentar o tamanho do filho de cada lado"

#: ../btk/btkbbox.c:128
msgid "Child internal height padding"
msgstr "Altura de espaçamento interno do filho"

#: ../btk/btkbbox.c:129
msgid "Amount to increase child's size on the top and bottom"
msgstr "Quanto aumentar o tamanho do filho no topo e fundo"

#: ../btk/btkbbox.c:137
msgid "Layout style"
msgstr "Estilo de disposição"

#: ../btk/btkbbox.c:138
msgid ""
"How to lay out the buttons in the box. Possible values are: default, spread, "
"edge, start and end"
msgstr ""
"Como dispor os botões na caixa. Os valores possíveis são: por omissão, "
"afastados, extremos, início e fim"

#: ../btk/btkbbox.c:146
msgid "Secondary"
msgstr "Secundário"

#: ../btk/btkbbox.c:147
msgid ""
"If TRUE, the child appears in a secondary group of children, suitable for, e."
"g., help buttons"
msgstr ""
"Se VERDADEIRO, o filho aparece num grupo secundário de filhos, adequado a, "
"por ex., botões de ajuda"

#: ../btk/btkbox.c:130 ../btk/btkexpander.c:221 ../btk/btkiconview.c:666
#: ../btk/btktreeviewcolumn.c:217
msgid "Spacing"
msgstr "Espaçamento"

#: ../btk/btkbox.c:131
msgid "The amount of space between children"
msgstr "A quantidade de espaço entre filhos"

#: ../btk/btkbox.c:140 ../btk/btknotebook.c:657 ../btk/btktable.c:165
#: ../btk/btktoolbar.c:573 ../btk/btktoolitemgroup.c:1596
msgid "Homogeneous"
msgstr "Homogéneo"

#: ../btk/btkbox.c:141
msgid "Whether the children should all be the same size"
msgstr "Se os filhos deverão ou não ter todos o mesmo tamanho"

#: ../btk/btkbox.c:148 ../btk/btkpreview.c:102 ../btk/btktoolbar.c:565
#: ../btk/btktoolitemgroup.c:1603 ../btk/btktoolpalette.c:1053
#: ../btk/btktreeviewcolumn.c:273
msgid "Expand"
msgstr "Expandir"

#: ../btk/btkbox.c:149
msgid "Whether the child should receive extra space when the parent grows"
msgstr "Se o filho deverá ou não receber espaço extra quando o pai cresce"

#: ../btk/btkbox.c:155 ../btk/btktoolitemgroup.c:1610
msgid "Fill"
msgstr "Preencher"

#: ../btk/btkbox.c:156
msgid ""
"Whether extra space given to the child should be allocated to the child or "
"used as padding"
msgstr ""
"Se o espaço extra dado ao filho deverá ser alocado ao filho ou utilizado "
"como espaçamento"

#: ../btk/btkbox.c:162
msgid "Padding"
msgstr "Espaçamento"

#: ../btk/btkbox.c:163
msgid "Extra space to put between the child and its neighbors, in pixels"
msgstr "Espaço extra para colocar entre o filho e os seus vizinhos, em pixels"

#: ../btk/btkbox.c:169
msgid "Pack type"
msgstr "Tipo de arrumação"

#: ../btk/btkbox.c:170 ../btk/btknotebook.c:724
msgid ""
"A BtkPackType indicating whether the child is packed with reference to the "
"start or end of the parent"
msgstr ""
