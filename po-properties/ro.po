#: ../bdk/bdkdisplaymanager.c:103
msgid "Default Display"
msgstr "Display implicit"

#: ../bdk/bdkdisplaymanager.c:104
msgid "The default display for BDK"
msgstr "Display implicit pentru BDK"

#: ../bdk/bdkbango.c:538 ../btk/btkinvisible.c:86
#: ../btk/btkmountoperation.c:176 ../btk/btkstatusicon.c:281
#: ../btk/btkwindow.c:641
msgid "Screen"
msgstr "Ecran"

#: ../bdk/bdkbango.c:539
msgid "the BdkScreen for the renderer"
msgstr "valoare BdkScreen pentru afișare"

#: ../bdk/bdkscreen.c:75
msgid "Font options"
msgstr "Opțiuni fonturi"

#: ../bdk/bdkscreen.c:76
msgid "The default font options for the screen"
msgstr "Opțiunile implicite pentru ecran ale fonturilor"

#: ../bdk/bdkscreen.c:83
msgid "Font resolution"
msgstr "Rezoluție fonturi"

#: ../bdk/bdkscreen.c:84
msgid "The resolution for fonts on the screen"
msgstr "Rezoluția fonturilor pentru ecran"

#: ../bdk/bdkwindow.c:496 ../bdk/bdkwindow.c:497
msgid "Cursor"
msgstr "Cursor"

#: ../btk/btkaboutdialog.c:332
msgid "Program name"
msgstr "Nume program"

#: ../btk/btkaboutdialog.c:333
msgid ""
"The name of the program. If this is not set, it defaults to "
"g_get_application_name()"
msgstr ""
"Numele programului. În cazul în care nu e inițializat, se "
"utilizează g_get_application_name()"

#: ../btk/btkaboutdialog.c:347
msgid "Program version"
msgstr "Versiune program"

#: ../btk/btkaboutdialog.c:348
msgid "The version of the program"
msgstr "Versiunea programului"

#: ../btk/btkaboutdialog.c:362
msgid "Copyright string"
msgstr "Drepturi de autor"

#: ../btk/btkaboutdialog.c:363
msgid "Copyright information for the program"
msgstr "Drepturile de autor pentru acest program"

#: ../btk/btkaboutdialog.c:380
msgid "Comments string"
msgstr "Alte informații"

#: ../btk/btkaboutdialog.c:381
msgid "Comments about the program"
msgstr "Informații despre program"

#: ../btk/btkaboutdialog.c:415
msgid "Website URL"
msgstr "Adresă website"

#: ../btk/btkaboutdialog.c:416
msgid "The URL for the link to the website of the program"
msgstr "Adresa website-ului programului"

#: ../btk/btkaboutdialog.c:431
msgid "Website label"
msgstr "Etichetă website"

#: ../btk/btkaboutdialog.c:432
msgid ""
"The label for the link to the website of the program. If this is not set, it "
"defaults to the URL"
msgstr ""
"Eticheta pentru adresa website-ului programului. Dacă nu e inițializată, se "
"utilizează adresa"

#: ../btk/btkaboutdialog.c:448
msgid "Authors"
msgstr "Autori"

#: ../btk/btkaboutdialog.c:449
msgid "List of authors of the program"
msgstr "Lista autorilor programului"

#: ../btk/btkaboutdialog.c:465
msgid "Documenters"
msgstr "Documentariști"

#: ../btk/btkaboutdialog.c:466
msgid "List of people documenting the program"
msgstr "Lista persoanelor ce au documentat programul"

#: ../btk/btkaboutdialog.c:482
msgid "Artists"
msgstr "Artiști"

#: ../btk/btkaboutdialog.c:483
msgid "List of people who have contributed artwork to the program"
msgstr "Lista persoanelor cu contribuții artistice la program"

#: ../btk/btkaboutdialog.c:500
msgid "Translator credits"
msgstr "Traducători"

#: ../btk/btkaboutdialog.c:501
msgid ""
"Credits to the translators. This string should be marked as translatable"
msgstr "Lista traducătorilor. Acest șir trebuie marcat ca traductibil"

#: ../btk/btkaboutdialog.c:516
msgid "Logo"
msgstr "Siglă"

#: ../btk/btkaboutdialog.c:517
msgid ""
"A logo for the about box. If this is not set, it defaults to "
"btk_window_get_default_icon_list()"
msgstr ""
"O siglă pentru fereastra „Despre”. Dacă nu există, se va utiliza "
"btk_window_get_default_icon_list()"

#: ../btk/btkaboutdialog.c:532
msgid "Logo Icon Name"
msgstr "Numele iconiței siglă"

#: ../btk/btkaboutdialog.c:533
msgid "A named icon to use as the logo for the about box."
msgstr "O iconiță desemnată ca siglă a programului."

#: ../btk/btkaboutdialog.c:546
msgid "Wrap license"
msgstr "Rupe rândurile licenței"

#: ../btk/btkaboutdialog.c:547
msgid "Whether to wrap the license text."
msgstr "Specifică dacă textul licenței să aibă rândurile rupte."

#: ../btk/btkaccellabel.c:178
msgid "Accelerator Closure"
msgstr "Închidere accelerator"

#: ../btk/btkaccellabel.c:179
msgid "The closure to be monitored for accelerator changes"
msgstr "Închiderea de monitorizat pentru schimbări ale acceleratorului"

#: ../btk/btkaccellabel.c:185
msgid "Accelerator Widget"
msgstr "Widget accelerator"

#: ../btk/btkaccellabel.c:186
msgid "The widget to be monitored for accelerator changes"
msgstr "Widget de monitorizat pentru schimbări ale acceleratorului"

#: ../btk/btkaction.c:220 ../btk/btkactiongroup.c:170 ../btk/btkprinter.c:111
#: ../btk/btktextmark.c:89
msgid "Name"
msgstr "Nume"

#: ../btk/btkaction.c:221
msgid "A unique name for the action."
msgstr "Un nume unic pentru acțiune."

#: ../btk/btkaction.c:239 ../btk/btkbutton.c:219 ../btk/btkexpander.c:197
#: ../btk/btkframe.c:105 ../btk/btklabel.c:507 ../btk/btkmenuitem.c:305
#: ../btk/btktoolbutton.c:204 ../btk/btktoolitemgroup.c:1543
msgid "Label"
msgstr "Etichetă"

#: ../btk/btkaction.c:240
msgid "The label used for menu items and buttons that activate this action."
msgstr ""
"Etichetă utilizată pentru elementele din meniu și butoanele ce activează "
"această acțiune."

#: ../btk/btkaction.c:256
msgid "Short label"
msgstr "Etichetă scurtă"

#: ../btk/btkaction.c:257
msgid "A shorter label that may be used on toolbar buttons."
msgstr "O etichetă mai scurtă de utilizat pentru butoanele din bara cu unelte."

#: ../btk/btkaction.c:265
msgid "Tooltip"
msgstr "Indiciu"

#: ../btk/btkaction.c:266
msgid "A tooltip for this action."
msgstr "Un indiciu pentru această acțiune."

#: ../btk/btkaction.c:281
msgid "Stock Icon"
msgstr "Iconiță standard"

#: ../btk/btkaction.c:282
msgid "The stock icon displayed in widgets representing this action."
msgstr "Iconița standard de afișat într-un widget asociat acelei acțiuni."

#: ../btk/btkaction.c:302 ../btk/btkstatusicon.c:254
msgid "GIcon"
msgstr "GIcon"

#: ../btk/btkaction.c:303 ../btk/btkcellrendererpixbuf.c:206
#: ../btk/btkimage.c:341 ../btk/btkstatusicon.c:255
msgid "The GIcon being displayed"
msgstr "Elementul GIcon afișat"

#: ../btk/btkaction.c:323 ../btk/btkcellrendererpixbuf.c:171
#: ../btk/btkimage.c:323 ../btk/btkprinter.c:160 ../btk/btkstatusicon.c:238
#: ../btk/btkwindow.c:633
msgid "Icon Name"
msgstr "Nume iconiță"

#: ../btk/btkaction.c:324 ../btk/btkcellrendererpixbuf.c:172
#: ../btk/btkimage.c:324 ../btk/btkstatusicon.c:239
msgid "The name of the icon from the icon theme"
msgstr "Numele iconiței din tema de iconițe"

#: ../btk/btkaction.c:331 ../btk/btktoolitem.c:192
msgid "Visible when horizontal"
msgstr "Vizibil pe orizontală"

#: ../btk/btkaction.c:332 ../btk/btktoolitem.c:193
msgid ""
"Whether the toolbar item is visible when the toolbar is in a horizontal "
"orientation."
msgstr ""
"Specifică dacă elementul din bara cu unelte va fi vizibil când bara cu "
"unelte este orientată pe orizontală."

#: ../btk/btkaction.c:347
msgid "Visible when overflown"
msgstr "Vizibil întotdeauna"

#: ../btk/btkaction.c:348
msgid ""
"When TRUE, toolitem proxies for this action are represented in the toolbar "
"overflow menu."
msgstr ""
"La activare, elementele din bara de unelte pentru această acțiune vor fi "
"afișate în meniul suplimentar când spațiul e insuficient."

#: ../btk/btkaction.c:355 ../btk/btktoolitem.c:199
msgid "Visible when vertical"
msgstr "Vizibil pe verticală"

#: ../btk/btkaction.c:356 ../btk/btktoolitem.c:200
msgid ""
"Whether the toolbar item is visible when the toolbar is in a vertical "
"orientation."
msgstr ""
"Specifică dacă elementul din bara cu unelte va fi vizibil când bara cu "
"unelte este orientată pe verticală."

#: ../btk/btkaction.c:363 ../btk/btktoolitem.c:206
msgid "Is important"
msgstr "E important"

#: ../btk/btkaction.c:364
msgid ""
"Whether the action is considered important. When TRUE, toolitem proxies for "
"this action show text in BTK_TOOLBAR_BOTH_HORIZ mode."
msgstr ""
"Specifică dacă acțiunea este considerată importantă. La activare, se va "
"afișa un text lângă această iconiță în modul BTK_TOOLBAR_BOTH_HORIZ."

#: ../btk/btkaction.c:372
msgid "Hide if empty"
msgstr "Ascunde dacă e gol"

#: ../btk/btkaction.c:373
msgid "When TRUE, empty menu proxies for this action are hidden."
msgstr "La activare, se ascund intrările nule pentru această acțiune"

#: ../btk/btkaction.c:379 ../btk/btkactiongroup.c:177
#: ../btk/btkcellrenderer.c:193 ../btk/btkwidget.c:594
msgid "Sensitive"
msgstr "Senzitivă"

#: ../btk/btkaction.c:380
msgid "Whether the action is enabled."
msgstr "Specifică dacă acțiunea este vizibilă"

#: ../btk/btkaction.c:386 ../btk/btkactiongroup.c:184
#: ../btk/btkstatusicon.c:304 ../btk/btktreeviewcolumn.c:192
#: ../btk/btkwidget.c:587
msgid "Visible"
msgstr "Vizibilă"

#: ../btk/btkaction.c:387
msgid "Whether the action is visible."
msgstr "Specifică dacă acțiunea este vizibilă"

#: ../btk/btkaction.c:393
msgid "Action Group"
msgstr "Grup de acțiuni"

#: ../btk/btkaction.c:394
msgid ""
"The BtkActionGroup this BtkAction is associated with, or NULL (for internal "
"use)."
msgstr ""
"Elemntul BtkActionGroup cu care acest BtkAction este asociat, sau NULL "
"(pentru uz intern)."

#: ../btk/btkaction.c:412 ../btk/btkimagemenuitem.c:169
msgid "Always show image"
msgstr "Arată întotdeauna imaginea"

#: ../btk/btkaction.c:413 ../btk/btkimagemenuitem.c:170
msgid "Whether the image will always be shown"
msgstr "Specifică dacă imaginea este întotdeauna afișată"

#: ../btk/btkactiongroup.c:171
msgid "A name for the action group."
msgstr "Un nume pentru grupul de acțiuni"

#: ../btk/btkactiongroup.c:178
msgid "Whether the action group is enabled."
msgstr "Specifică dacă grupul de acțiuni este activat."

#: ../btk/btkactiongroup.c:185
msgid "Whether the action group is visible."
msgstr "Specifică dacă grupul de acțiuni este vizibil."

#: ../btk/btkactivatable.c:308
msgid "Related Action"
msgstr "Acțiune înrudită"

#: ../btk/btkactivatable.c:309
msgid "The action this activatable will activate and receive updates from"
msgstr ""
"Acțiune pe care acest element activabil o va activa și pentru care va primi "
"actualizări"

#: ../btk/btkactivatable.c:331
msgid "Use Action Appearance"
msgstr "Utilizează ilustrarea acțiunii"

#: ../btk/btkactivatable.c:332
msgid "Whether to use the related actions appearance properties"
msgstr ""
"Specifică dacă se utilizează proprietățile ilustrărilor pentru acțiunile "
"înrudite"

#: ../btk/btkadjustment.c:93 ../btk/btkcellrendererprogress.c:128
#: ../btk/btkscalebutton.c:206 ../btk/btkspinbutton.c:269
msgid "Value"
msgstr "Valoare"

#: ../btk/btkadjustment.c:94
msgid "The value of the adjustment"
msgstr "Valuarea ajustării"

#: ../btk/btkadjustment.c:110
msgid "Minimum Value"
msgstr "Valoare minimă"

#: ../btk/btkadjustment.c:111
msgid "The minimum value of the adjustment"
msgstr "Valoarea minimă a ajustării"

#: ../btk/btkadjustment.c:130
msgid "Maximum Value"
msgstr "Valoare maximă"

#: ../btk/btkadjustment.c:131
msgid "The maximum value of the adjustment"
msgstr "Valoarea maximă a ajustării"

#: ../btk/btkadjustment.c:147
msgid "Step Increment"
msgstr "Pas de incrementare"

#: ../btk/btkadjustment.c:148
msgid "The step increment of the adjustment"
msgstr "Pasul incrementării la ajustare"

#: ../btk/btkadjustment.c:164
msgid "Page Increment"
msgstr "Incrementare pagină"

#: ../btk/btkadjustment.c:165
msgid "The page increment of the adjustment"
msgstr "Incrementarea paginii la ajustare"

#: ../btk/btkadjustment.c:184
msgid "Page Size"
msgstr "Mărime pagină"

#: ../btk/btkadjustment.c:185
msgid "The page size of the adjustment"
msgstr "Mărimea paginii la ajustare"

#: ../btk/btkalignment.c:109
msgid "Horizontal alignment"
msgstr "Aliniere orizontală"

#: ../btk/btkalignment.c:110 ../btk/btkbutton.c:270
msgid ""
"Horizontal position of child in available space. 0.0 is left aligned, 1.0 is "
"right aligned"
msgstr ""
"Poziția orizontală a copilului în spațiul disponibil. 0.0 înseamnă aliniere "
"la stânga, 1.0 înseamnă aliniere la dreapta"

#: ../btk/btkalignment.c:119
msgid "Vertical alignment"
msgstr "Aliniere verticală"

#: ../btk/btkalignment.c:120 ../btk/btkbutton.c:289
msgid ""
"Vertical position of child in available space. 0.0 is top aligned, 1.0 is "
"bottom aligned"
msgstr ""
"Poziția verticală a copilului în spațiul disponibil. 0.0 înseamnă aliniere "
"sus, 1.0 înseamnă aliniere jos"

#: ../btk/btkalignment.c:128
msgid "Horizontal scale"
msgstr "Scalare orizontală"

#: ../btk/btkalignment.c:129
msgid ""
"If available horizontal space is bigger than needed for the child, how much "
"of it to use for the child. 0.0 means none, 1.0 means all"
msgstr ""
"Dacă spațiul disponibil orizontal este mai mare decât are nevoie copilul, "
"cât să fie utilizat pentru copil. 0.0 înseamnă nimic, 1.0 înseamnă tot"

#: ../btk/btkalignment.c:137
msgid "Vertical scale"
msgstr "Scalare verticală"

#: ../btk/btkalignment.c:138
msgid ""
"If available vertical space is bigger than needed for the child, how much of "
"it to use for the child. 0.0 means none, 1.0 means all"
msgstr ""
"Dacă spațiul disponibil vertical este mai mare decât copilul are nevoie, cât "
"să fie utilizat pentru copil. 0.0 înseamnă nimic, 1.0 înseamnă tot"

#: ../btk/btkalignment.c:155
msgid "Top Padding"
msgstr "Umplere deasupra"

#: ../btk/btkalignment.c:156
msgid "The padding to insert at the top of the widget."
msgstr "Spațiul de umplutură de inserat deasupra unui widget."

#: ../btk/btkalignment.c:172
msgid "Bottom Padding"
msgstr "Umplere dedesubt"

#: ../btk/btkalignment.c:173
msgid "The padding to insert at the bottom of the widget."
msgstr "Spațiul de umplutură de inserat dedesubtul unui widget."

#: ../btk/btkalignment.c:189
msgid "Left Padding"
msgstr "Umplere la stânga"

#: ../btk/btkalignment.c:190
msgid "The padding to insert at the left of the widget."
msgstr "Spațiul de umplutură de inserat la stânga unui widget."

#: ../btk/btkalignment.c:206
msgid "Right Padding"
msgstr "Umplere la dreapta"

#: ../btk/btkalignment.c:207
msgid "The padding to insert at the right of the widget."
msgstr "Spațiul de umplutură de inserat la dreapta unui widget."

#: ../btk/btkarrow.c:95
msgid "Arrow direction"
msgstr "Direcție săgeată"

#: ../btk/btkarrow.c:96
msgid "The direction the arrow should point"
msgstr "Direcția pe care săgeata ar trebui să o indice"

#: ../btk/btkarrow.c:104
msgid "Arrow shadow"
msgstr "Umbră săgeată"

#: ../btk/btkarrow.c:105
msgid "Appearance of the shadow surrounding the arrow"
msgstr "Aspectul umbrei care înconjoară săgeata"

#: ../btk/btkarrow.c:112 ../btk/btkmenu.c:717 ../btk/btkmenuitem.c:368
msgid "Arrow Scaling"
msgstr "Redimensionare săgeată"

#: ../btk/btkarrow.c:113
msgid "Amount of space used up by arrow"
msgstr "Spațiu ocupat de săgeată"

#: ../btk/btkaspectframe.c:93
msgid "Horizontal Alignment"
msgstr "Aliniere orizontală"

#: ../btk/btkaspectframe.c:94
msgid "X alignment of the child"
msgstr "Alinierea pe axa X a copilului"

#: ../btk/btkaspectframe.c:100
msgid "Vertical Alignment"
msgstr "Aliniere verticală"

#: ../btk/btkaspectframe.c:101
msgid "Y alignment of the child"
msgstr "Alinierea pe axa Y a copilului"

#: ../btk/btkaspectframe.c:107
msgid "Ratio"
msgstr "Proporție"

#: ../btk/btkaspectframe.c:108
msgid "Aspect ratio if obey_child is FALSE"
msgstr "Proporția dacă „obey_child” este FALSE"

#: ../btk/btkaspectframe.c:114
msgid "Obey child"
msgstr "Potrivește după copil"

#: ../btk/btkaspectframe.c:115
msgid "Force aspect ratio to match that of the frame's child"
msgstr "Forțează proporția să se potrivească cu copilul cadrului"

#: ../btk/btkassistant.c:308
msgid "Header Padding"
msgstr "Umplere antet"

#: ../btk/btkassistant.c:309
msgid "Number of pixels around the header."
msgstr "Numărul pixelilor din jurul antetului."

#: ../btk/btkassistant.c:316
msgid "Content Padding"
msgstr "Umplere conținut"

#: ../btk/btkassistant.c:317
msgid "Number of pixels around the content pages."
msgstr "Numărul pixelilor din jurul conținutului."

#: ../btk/btkassistant.c:333
msgid "Page type"
msgstr "Tip pagină"

#: ../btk/btkassistant.c:334
msgid "The type of the assistant page"
msgstr "Tipul paginii asistentului"

#: ../btk/btkassistant.c:351
msgid "Page title"
msgstr "Titlu pagină"

#: ../btk/btkassistant.c:352
msgid "The title of the assistant page"
msgstr "Titlul paginii asistentului"

#: ../btk/btkassistant.c:368
msgid "Header image"
msgstr "Imagine antet"

#: ../btk/btkassistant.c:369
msgid "Header image for the assistant page"
msgstr "Imagine antet pentru pagina asistentului"

#: ../btk/btkassistant.c:385
msgid "Sidebar image"
msgstr "Imagine laterală"

#: ../btk/btkassistant.c:386
msgid "Sidebar image for the assistant page"
msgstr "Imagine laterală pentru pagina asistentului"

#: ../btk/btkassistant.c:401
msgid "Page complete"
msgstr "Pagină completă"

#: ../btk/btkassistant.c:402
msgid "Whether all required fields on the page have been filled out"
msgstr "Specifică dacă toate câmpurile din pagină au fost completate"

#: ../btk/btkbbox.c:101
msgid "Minimum child width"
msgstr "Lățime minimă copil"

#: ../btk/btkbbox.c:102
msgid "Minimum width of buttons inside the box"
msgstr "Lățimea minimă a butoanelor înăuntrul cutiei"

#: ../btk/btkbbox.c:110
msgid "Minimum child height"
msgstr "Înălțime minimă copil"

#: ../btk/btkbbox.c:111
msgid "Minimum height of buttons inside the box"
msgstr "Înălțimea minimă a butoanelor înăuntrul cutiei"

#: ../btk/btkbbox.c:119
msgid "Child internal width padding"
msgstr "Umplere lățime internă copil"

#: ../btk/btkbbox.c:120
msgid "Amount to increase child's size on either side"
msgstr ""
"Cantitatea cu care să fie mărită dimensiunea copilului pe fiecare parte"

#: ../btk/btkbbox.c:128
msgid "Child internal height padding"
msgstr "Umplere înălțime internă a copilului"

#: ../btk/btkbbox.c:129
msgid "Amount to increase child's size on the top and bottom"
msgstr ""
"Cantitatea cu care să fie mărită dimensiunea copilului deasupra și dedesubt"

#: ../btk/btkbbox.c:137
msgid "Layout style"
msgstr "Stilul aspectului"

#: ../btk/btkbbox.c:138
msgid ""
"How to lay out the buttons in the box. Possible values are: default, spread, "
"edge, start and end"
msgstr ""
"Mod de aranjare a butoanelor în cutie. Valorile posibile sunt: „default”, "
"„spread”, „edge”, „start” și „end”."

#: ../btk/btkbbox.c:146
msgid "Secondary"
msgstr "Secundar"

#: ../btk/btkbbox.c:147
msgid ""
"If TRUE, the child appears in a secondary group of children, suitable for, e."
"g., help buttons"
msgstr ""
"La activare, copilul apare într-un grup secundar de copii, cum e cazul "
"butoanelor de ajutor, de exemplu"

#: ../btk/btkbox.c:130 ../btk/btkexpander.c:221 ../btk/btkiconview.c:666
#: ../btk/btktreeviewcolumn.c:217
msgid "Spacing"
msgstr "Spațiere"

#: ../btk/btkbox.c:131
msgid "The amount of space between children"
msgstr "Cantitatea de spațiu între copii"

#: ../btk/btkbox.c:140 ../btk/btknotebook.c:658 ../btk/btktable.c:165
#: ../btk/btktoolbar.c:575 ../btk/btktoolitemgroup.c:1596
msgid "Homogeneous"
msgstr "Omogen"

#: ../btk/btkbox.c:141
msgid "Whether the children should all be the same size"
msgstr "Specifică dacă copiii ar trebui să fie toți de aceeași mărime"

#: ../btk/btkbox.c:148 ../btk/btkpreview.c:102 ../btk/btktoolbar.c:567
#: ../btk/btktoolitemgroup.c:1603 ../btk/btktoolpalette.c:1053
#: ../btk/btktreeviewcolumn.c:273
msgid "Expand"
msgstr "Extinde"

#: ../btk/btkbox.c:149
msgid "Whether the child should receive extra space when the parent grows"
msgstr ""
"Specifică dacă un copil ar trebui să primească spațiu în plus când părintele "
"crește"

#: ../btk/btkbox.c:155 ../btk/btktoolitemgroup.c:1610
msgid "Fill"
msgstr "Umplere"

#: ../btk/btkbox.c:156
msgid ""
"Whether extra space given to the child should be allocated to the child or "
"used as padding"
