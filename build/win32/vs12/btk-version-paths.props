<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <VSVer>12</VSVer>
    <BunnylibEtcInstallRoot>..\..\..\..\vs$(VSVer)\$(Platform)</BunnylibEtcInstallRoot>
    <CopyDir>$(BunnylibEtcInstallRoot)</CopyDir>
    <DefDir>$(SolutionDir)$(Configuration)\$(Platform)\obj\$(ProjectName)</DefDir>
    <ApiVersion>2.0</ApiVersion>
    <BtkVersion>2.24.33</BtkVersion>
    <BtkLibtoolCompatibleDllPrefix>lib</BtkLibtoolCompatibleDllPrefix>
    <BtkLibtoolCompatibleDllSuffix>-$(ApiVersion)-0</BtkLibtoolCompatibleDllSuffix>
    <BtkSeparateVSDllPrefix />
    <BtkSeparateVSDllSuffix>-2-vs$(VSVer)</BtkSeparateVSDllSuffix>
    <!-- Change these two to BtkLibtoolCompatibleDllPrefix and
	     BtkLibtoolCompatibleDllSuffix if that is what you want -->
    <BtkDllPrefix>$(BtkSeparateVSDllPrefix)</BtkDllPrefix>
    <BtkDllSuffix>$(BtkSeparateVSDllSuffix)</BtkDllSuffix>
    <!-- Update the following to reflect the actual Python 2.x/3.x installation path on your system -->
    <PythonPath>c:\python34</PythonPath>
    <PythonPathX64>$(PythonPath).x64</PythonPathX64>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>btkversionpathsprops</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="VSVer">
      <Value>$(VSVer)</Value>
    </BuildMacro>
    <BuildMacro Include="BunnylibEtcInstallRoot">
      <Value>$(BunnylibEtcInstallRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="CopyDir">
      <Value>$(CopyDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DefDir">
      <Value>$(DefDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ApiVersion">
      <Value>$(ApiVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkVersion">
      <Value>$(BtkVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkLibtoolCompatibleDllPrefix">
      <Value>$(BtkLibtoolCompatibleDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkLibtoolCompatibleDllSuffix">
      <Value>$(BtkLibtoolCompatibleDllSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkSeparateVSDllPrefix">
      <Value>$(BtkSeparateVSDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkSeparateVSDllSuffix">
      <Value>$(BtkSeparateVSDllSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkDllPrefix">
      <Value>$(BtkDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="BtkDllSuffix">
      <Value>$(BtkDllSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="PythonPath">
      <Value>$(PythonPath)</Value>
    </BuildMacro>
    <BuildMacro Include="PythonPathX64">
      <Value>$(PythonPathX64)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
